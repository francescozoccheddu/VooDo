<Application
    x:Class="VooDo.Testbench.Desktop.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls"/>
                <ResourceDictionary>
                    <Color x:Key="m_bgAccent">#DB0045</Color>
                    <Color x:Key="m_fgAccent">White</Color>
                    <Color x:Key="m_fgAccentM">#FF4782</Color>
                    <Color x:Key="m_fgAccentL">#FA3874</Color>
                    <Color x:Key="m_bgAccentV1">#F7617E</Color>
                    <Color x:Key="m_bgAccentV2">#FF5CAF</Color>
                    <Color x:Key="m_bg">#D0D0D0</Color>
                    <Color x:Key="m_fg">#303030</Color>
                    <Color x:Key="m_fgM">#404040</Color>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
